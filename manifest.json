{
    "name": "Manhattan 24-hour healthcare accessibility",
    "description": "Measuring accessibility to healthcare resources over 24 hours",
    "estimated_runtime": "6 hours",
    "container": "cybergisx-0.4",
    "pre_processing_stage": "bash update_osmnx.sh",
    "execution_stage": "python measure_access.py",
    "slurm_input_rules": {
           "time": {
                "max": 10,
                "min": 10,
                "default_value": 10,
                "step": 1,
                "unit": "Hours"
            },
            "memory_per_cpu": {
                "max": 16,
                "min": 1,
                "default_value": 8,
                "step": 1,
                "unit": "GB"
            },
            "num_of_task": {
                "max": 48,
                "min": 16,
                "default_value": 16,
                "step": 8
            }
    },
    "require_upload_data": false,
    "supported_hpc": ["keeling_community", "expanse_community", "anvil_community"],
    "default_hpc": "keeling_community"
}